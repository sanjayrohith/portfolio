{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Install new dependencies",
			"type": "shell",
			"command": "npm install",
			"args": [],
			"isBackground": false,
			"problemMatcher": [
				"$npm-console"
			],
			"group": "build"
		},
		{
			"label": "Check git status",
			"type": "shell",
			"command": "git status --porcelain=v1",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "typecheck",
			"type": "shell",
			"command": "npm run typecheck",
			"isBackground": false,
			"group": "build"
		}
	]
}